    SQL.Strings = (
      'Select  diag.name diagnos'
      '       ,pac.date_birth Data'
      '       ,count(visit.diagnos) CountDaignos'
      #9'   ,count(pac.date_birth) countDate'
      #9'   '#9'   '
      
        '  from Visit left outer join diagnoses diag on Visit.diagnos=dia' +
        'g.id'
      '             left outer join pac on visit.pac=pac.id'
