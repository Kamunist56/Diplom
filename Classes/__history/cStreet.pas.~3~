unit cStreet;

interface
type
  Tstreet = Class
    public
    id:integer;
    name:string;

   Constructor Create(Find_id:integer);

  End;
implementation

{ Tstreet }

uses fDM;

constructor Tstreet.Create(Find_id: integer);
begin
    dm.QGetStreet.Close;
    dm.QGetStreet.Parameters.ParamValues['id']:=Find_id;
    dm.QGetStreet.Open;
    name:=dm.QGetStreetname.AsString;
    id:=find_id;
end;

end.
